    "def round_sum(a, b, c):\n",
    "  return round10(a) + round10(b) + round10(c)\n",
    "    \n",
    "def round10(num):\n",
    "  if num % 10 >= 5:\n",
    "    return num + (10 - num % 10)\n",
    "  return num - (num % 10)\n",
    "\n",
    "\n",
    "round_sum(10, 8, 27)\n"
   ]
